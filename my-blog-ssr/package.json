{
  "name": "myblog",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "serve": "cross-env NODE_ENV=development node server.js",
    "dev:ssr": "cross-env NODE_ENV=development node server.js",
    "prod:ssr": "cross-env NODE_ENV=production node server.js",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.ts",
    "build": "npm run build:client && npm run build:server",
    "preview": "vite preview",
    "lint:prettier": "prettier {src,mock}/**/*.{html,vue,css,sass,scss,ts,md} --write",
    "lint:eslint": "eslint {library,src}/**/*.{vue,ts,tsx} --fix",
    "prepare": "husky install",
    "lint": "eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx",
    "test": "jest"
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  },
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "@kangc/v-md-editor": "^2.3.15",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/server-renderer": "^3.2.33",
    "axios": "^0.26.1",
    "element-plus": "^2.1.8",
    "koa": "^2.13.4",
    "koa-connect": "^2.1.0",
    "koa-send": "^5.0.1",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "vue": "^3.2.25",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/jest": "^27.4.1",
    "@types/koa": "^2.13.4",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.23",
    "@types/nprogress": "^0.2.0",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@vitejs/plugin-vue": "^2.3.0",
    "@vue/cli-plugin-babel": "^5.0.4",
    "@vue/test-utils": "^2.0.0-rc.18",
    "babel-jest": "26.3.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.6.0",
    "husky": "^7.0.0",
    "jest": "26.6.3",
    "less": "^4.1.2",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.6.3",
    "unplugin-auto-import": "^0.6.9",
    "unplugin-vue-components": "^0.18.5",
    "vite": "^2.9.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-global-api": "^0.4.1",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "^0.29.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "moduleFileExtensions": [
    "js",
    "jsx",
    "json",
    "vue"
  ]
}
